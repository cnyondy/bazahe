<?xml version="1.0" encoding="UTF-8"?>

<?import bazahe.ui.pane.WebSocketMessagePane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<fx:root type="bazahe.ui.pane.WebSocketMessagePane" xmlns:fx="http://javafx.com/fxml"
         fx:controller="bazahe.ui.controller.WebSocketMessageController"
         fx:id="root">
    <top>
        <HBox spacing="4" alignment="CENTER_LEFT">
            <padding>
                <Insets top="4" bottom="2" left="4" right="4"/>
            </padding>
            <Text fx:id="description"/>
            <Button onAction="#exportBody">Save Body</Button>
        </HBox>
    </top>
    <center>
    </center>
</fx:root>
